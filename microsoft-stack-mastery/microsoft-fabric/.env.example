# Microsoft Fabric Examples - Environment Variables Template
# Copy this file to .env and fill in your values

# Azure/Fabric Configuration
TENANT_ID=your-tenant-id
CLIENT_ID=your-client-id
CLIENT_SECRET=your-client-secret
SUBSCRIPTION_ID=your-subscription-id

# Fabric Workspace Configuration
WORKSPACE_ID=your-workspace-id
WORKSPACE_NAME=your-workspace-name
CAPACITY_ID=your-capacity-id

# Lakehouse Configuration
LAKEHOUSE_ID=your-lakehouse-id
LAKEHOUSE_NAME=your-lakehouse-name

# Warehouse Configuration
WAREHOUSE_ID=your-warehouse-id
WAREHOUSE_NAME=your-warehouse-name

# Storage Configuration (for lakehouse shortcuts)
ADLS_ACCOUNT_NAME=your-adls-account
ADLS_CONTAINER_NAME=your-container
ADLS_SAS_TOKEN=your-sas-token

# S3 Configuration (optional)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# API Configuration
FABRIC_API_BASE_URL=https://api.fabric.microsoft.com/v1

# Database Connection (for warehouse operations)
WAREHOUSE_CONNECTION_STRING=Driver={ODBC Driver 18 for SQL Server};Server=your-warehouse.datawarehouse.fabric.microsoft.com;Database=your-warehouse;Encrypt=yes;TrustServerCertificate=no;

# Logging Configuration
LOG_LEVEL=INFO
